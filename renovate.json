{
  "extends": [
    "github>browniebroke/renovate-configs:python",
    ":semanticCommitTypeAll(chore)"
  ],
  "automergeType": "branch",
  "packageRules": [
    {
      "groupName": "httpx",
      "matchPackageNames": [
        "*httpx*"
      ]
    },
    {
      "matchDepTypes": [
        "dependencies"
      ],
      "major": {
        "semanticCommitType": "fix"
      }
    }
  ]
}
